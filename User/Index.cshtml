<div ng-app="myapp">  
    <div ng-controller="UserCtrl">  
        <table class="table table-striped table-bordered table-hover table-checkable datatable">  
            <thead class="grid-top-panel">  
                <tr>  
                    <th>UserID</th>  
                    <th>First Name</th>  
                    <th>LastName</th>  
                    <th>Address</th>  
                </tr>  
            </thead>  
            <tbody>  
                <tr ng-repeat="dataModel in users">  
                    <td>{{dataModel.UserId}}</td>  
                    <td>{{dataModel.Name}}</td>  
                    <td>{{dataModel.FullName}}</td>  
                    <td>{{dataModel.Address}}</td>  
                </tr>  
            </tbody>  
        </table>  
    </div>  
</div>  

<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
<script src="~/NgApp/app.js"></script>
<script src="~/NgApp/services/UserService.js"></script>
<script src="~/NgApp/controllers/userController.js"></script>



